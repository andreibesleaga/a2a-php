{
  "name": "andreibesleaga/a2a-php",
  "description": "A2A (AI Agent-to-Agent) Protocol PHP SDK",
  "type": "library",
  "keywords": ["a2a", "agent", "protocol", "sdk"],
  "license": "Apache-2.0",
  "homepage": "https://github.com/andreibesleaga/a2a-php",
  "support": {
    "issues": "https://github.com/andreibesleaga/a2a-php/issues",
    "source": "https://github.com/andreibesleaga/a2a-php"
  },
  "authors": [
    {
      "name": "Andrei Besleaga"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ramsey/uuid": "^4.0",
    "psr/log": "^3.0",
    "monolog/monolog": "^3.0",
    "grpc/grpc": "^1.57",
    "illuminate/cache": "^12.20",
    "illuminate/filesystem": "^12.20",
    "illuminate/container": "^12.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "A2A\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "A2A\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src",
    "style": "phpcs src tests"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
