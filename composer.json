{
  "name": "a2a/a2a-php",
  "description": "A2A (Agent-to-Agent) Protocol PHP SDK",
  "type": "library",
  "keywords": ["a2a", "agent", "protocol", "sdk"],
  "license": "MIT",
  "authors": [
    {
      "name": "A2A Team",
      "email": "team@a2a.org"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "ramsey/uuid": "^4.0",
    "psr/log": "^3.0",
    "monolog/monolog": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "A2A\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "A2A\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse src",
    "style": "phpcs src tests"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}